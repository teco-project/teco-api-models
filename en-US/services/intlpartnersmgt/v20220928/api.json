{
  "actions": {
    "AllocateCustomerCredit": {
      "document": "This API is used for a partner to set credit for a customer, such as increasing or lowering the credit and setting it to 0.\n1. The credit is valid permanently and will not be zeroed regularly.\n2. The customer's service will be suspended when its available credit is set to 0, so caution should be exercised with this operation.\n3. To prevent the customer from making new purchases without affecting their use of previously purchased products, the partner can set their available credit to 0 after obtaining the non-stop feature privilege from the channel manager.\n4. The set credit is an increment of the current available credit and cannot exceed the remaining allocable credit. Setting the credit to a negative value indicates that it will be repossessed. The available credit can be set to 0 at the minimum.",
      "input": "AllocateCustomerCreditRequest",
      "name": "Allocates credit to customers by a partner",
      "output": "AllocateCustomerCreditResponse",
      "status": "online"
    },
    "CreateAccount": {
      "document": "This API is used to create a Tencent Cloud account on the partner platform for a customer. After registration, the customer will be automatically bound to the partner account.\n\nNotes:<br>\n1. The partner should verify the entered email address and mobile number for creating a Tencent Cloud account.<br>\n2. The customer needs to complete personal information after the first login.",
      "input": "CreateAccountRequest",
      "name": "Creates a customer account",
      "output": "CreateAccountResponse",
      "status": "online"
    },
    "GetCountryCodes": {
      "document": "This API is used to obtain country/region codes.",
      "input": "GetCountryCodesRequest",
      "name": "This API is used to obtain country/region codes.",
      "output": "GetCountryCodesResponse",
      "status": "online"
    },
    "QueryCreditAllocationHistory": {
      "document": "This API is used to query all the credit allocation records of a single customer.",
      "input": "QueryCreditAllocationHistoryRequest",
      "name": "Queries credit allocation records",
      "output": "QueryCreditAllocationHistoryResponse",
      "status": "online"
    },
    "QueryCreditByUinList": {
      "document": "This API is used to query the credit of users in the list.",
      "input": "QueryCreditByUinListRequest",
      "name": "Queries credit by UIN list",
      "output": "QueryCreditByUinListResponse",
      "status": "online"
    },
    "QueryCustomersCredit": {
      "document": "This API is used for a partner to the credits and basic information of cutomers.",
      "input": "QueryCustomersCreditRequest",
      "name": "Queries the credits of customers",
      "output": "QueryCustomersCreditResponse",
      "status": "online"
    },
    "QueryDirectCustomersCredit": {
      "document": "This API is used to query the credits of direct customers.",
      "input": "QueryDirectCustomersCreditRequest",
      "name": "Queries the credits of direct customers",
      "output": "QueryDirectCustomersCreditResponse",
      "status": "online"
    },
    "QueryPartnerCredit": {
      "document": "This API is used for a partner to query its own total credit, available credit, and used credit in USD.",
      "input": "QueryPartnerCreditRequest",
      "name": "Queries a partner's credit",
      "output": "QueryPartnerCreditResponse",
      "status": "online"
    },
    "QueryVoucherAmountByUin": {
      "document": "This API is used to query the voucher quota based on the customer UIN.",
      "input": "QueryVoucherAmountByUinRequest",
      "name": "Queries the voucher quota based on the customer UIN",
      "output": "QueryVoucherAmountByUinResponse",
      "status": "online"
    },
    "QueryVoucherListByUin": {
      "document": "This API is used to query the voucher list based on the customer UIN.",
      "input": "QueryVoucherListByUinRequest",
      "name": "Queries the voucher list based on the customer UIN",
      "output": "QueryVoucherListByUinResponse",
      "status": "online"
    },
    "QueryVoucherPool": {
      "document": "This API is used to query the voucher quota pool.",
      "input": "QueryVoucherPoolRequest",
      "name": "Queries the voucher quota pool",
      "output": "QueryVoucherPoolResponse",
      "status": "online"
    }
  },
