    "FuseFaceRequest": {
      "document": "FuseFace请求参数结构体",
      "members": [
        {
          "disabled": false,
          "document": "活动 ID，请在<a href=\"https://console.cloud.tencent.com/facefusion\" target=\"_blank\">人脸融合控制台</a>查看。",
          "example": "300029",
          "member": "string",
