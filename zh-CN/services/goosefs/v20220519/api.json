{
  "actions": {
    "CreateDataRepositoryTask": {
      "document": "创建数据流通任务,包括从将文件系统的数据上传到存储桶下, 以及从存储桶下载到文件系统里。",
      "input": "CreateDataRepositoryTaskRequest",
      "name": "创建数据流通任务",
      "output": "CreateDataRepositoryTaskResponse",
      "status": "online"
    },
    "DescribeDataRepositoryTaskStatus": {
      "document": "获取数据流通任务实时状态，用作客户端控制",
      "input": "DescribeDataRepositoryTaskStatusRequest",
      "name": "获取数据流通任务实时状态",
      "output": "DescribeDataRepositoryTaskStatusResponse",
      "status": "online"
    }
  },
  "metadata": {
    "apiVersion": "2022-05-19",
    "api_brief": "介绍如何使用API创建、删除、查询、管理数据加速器GooseFS，为GooseFS服务分发配置，管理存储在GooseFS服务中的文件等。",
    "serviceNameCN": "数据加速器 GooseFS",
    "serviceShortName": "goosefs"
  },
  "objects": {
    "CreateDataRepositoryTaskRequest": {
      "document": "CreateDataRepositoryTask请求参数结构体",
      "members": [
        {
          "disabled": false,
          "document": "数据流通任务类型, FS_TO_COS(文件系统到COS Bucket),或者COS_TO_FS(COS Bucket到文件系统)",
          "example": "FS_TO_BUCKET",
          "member": "string",
          "name": "TaskType",
          "required": true,
          "type": "string"
        },
        {
          "disabled": false,
          "document": "COS存储桶名",
          "example": "mybucket-1250000",
          "member": "string",
          "name": "Bucket",
          "required": true,
          "type": "string"
        },
        {
          "disabled": false,
          "document": "文件系统ID",
          "example": "x_c60_r3c4fa1f",
          "member": "string",
          "name": "FileSystemId",
          "required": true,
          "type": "string"
        },
        {
          "disabled": false,
          "document": "对于FS_TO_COS, TaskPath是Bucket映射目录的相对路径, 对于COS_TO_FS是COS上的路径。如果置为空, 则表示全部数据",
          "example": "aaa/bbb",
          "member": "string",
          "name": "TaskPath",
          "required": true,
          "type": "string"
        },
        {
          "disabled": false,
          "document": "任务名称",
          "example": "mydownload",
          "member": "string",
          "name": "TaskName",
          "required": true,
          "type": "string"
        },
        {
          "disabled": false,
          "document": "数据流通方式 MSP_AFM 手动加载  RAW_AFM 按需加载",
          "example": "MSP_AFM",
          "member": "string",
          "name": "RepositoryType",
          "required": false,
          "type": "string"
        },
        {
          "disabled": false,
          "document": "文件列表下载地址，以http开头",
          "example": "https://location.com/list.txt",
          "member": "string",
          "name": "TextLocation",
          "required": false,
          "type": "string"
        }
      ],
      "type": "object"
    },
    "CreateDataRepositoryTaskResponse": {
      "document": "CreateDataRepositoryTask返回参数结构体",
      "members": [
        {
          "disabled": false,
          "document": "任务ID",
          "example": "c1a2b3f4",
          "member": "string",
          "name": "TaskId",
          "type": "string",
          "value_allowed_null": false
        },
        {
          "document": "唯一请求 ID，每次请求都会返回。定位问题时需要提供该次请求的 RequestId。",
          "member": "string",
          "name": "RequestId",
          "type": "string"
        }
      ],
      "type": "object"
    },
    "DescribeDataRepositoryTaskStatusRequest": {
      "document": "DescribeDataRepositoryTaskStatus请求参数结构体",
      "members": [
        {
          "disabled": false,
          "document": "task id",
          "example": "x_task_r3c4fa1f",
          "member": "string",
          "name": "TaskId",
          "required": true,
          "type": "string"
        },
        {
          "disabled": false,
          "document": "file system id",
          "example": "x_c60_r3c4fa1f",
          "member": "string",
          "name": "FileSystemId",
          "required": true,
          "type": "string"
        }
      ],
      "type": "object"
    },
    "DescribeDataRepositoryTaskStatusResponse": {
      "document": "DescribeDataRepositoryTaskStatus返回参数结构体",
      "members": [
        {
          "disabled": false,
          "document": "任务id",
          "example": "x_task_r3c4fa1f",
          "member": "string",
          "name": "TaskId",
          "type": "string",
          "value_allowed_null": false
        },
        {
          "disabled": false,
          "document": "任务状态 0(初始化中), 1(运行中), 2(已完成), 3(任务失败)",
          "example": "1",
          "member": "int64",
          "name": "Status",
          "type": "int",
          "value_allowed_null": false
        },
        {
          "disabled": false,
          "document": "已完成的文件数量",
          "example": "0",
          "member": "uint64",
          "name": "FinishedFileNumber",
          "type": "int",
          "value_allowed_null": false
        },
        {
          "disabled": false,
          "document": "已完成的数据量",
          "example": "0",
          "member": "uint64",
          "name": "FinishedCapacity",
          "type": "int",
          "value_allowed_null": false
        },
        {
          "document": "唯一请求 ID，每次请求都会返回。定位问题时需要提供该次请求的 RequestId。",
          "member": "string",
          "name": "RequestId",
          "type": "string"
        }
      ],
      "type": "object"
    }
  },
  "version": "1.0"
}