    "CreateDataRepositoryTaskRequest": {
      "document": "CreateDataRepositoryTask请求参数结构体",
      "members": [
        {
          "disabled": false,
          "document": "数据流通任务类型, FS_TO_COS(文件系统到COS Bucket),或者COS_TO_FS(COS Bucket到文件系统)",
          "example": "FS_TO_BUCKET",
          "member": "string",
          "name": "TaskType",
          "required": true,
          "type": "string"
        },
        {
