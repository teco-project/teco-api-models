{
  "actions": {
    "CreateDataRepositoryTask": {
      "document": "创建数据流通任务,包括从将文件系统的数据上传到存储桶下, 以及从存储桶下载到文件系统里。",
      "input": "CreateDataRepositoryTaskRequest",
      "name": "创建数据流通任务",
      "output": "CreateDataRepositoryTaskResponse",
      "status": "online"
    },
    "DescribeClusterClientToken": {
      "document": "查询GooseFS集群客户端凭证",
      "input": "DescribeClusterClientTokenRequest",
      "name": "查询GooseFS集群客户端凭证",
      "output": "DescribeClusterClientTokenResponse",
      "status": "online"
    },
    "DescribeClusterRoleToken": {
      "document": "查询GooseFS集群角色凭证",
      "input": "DescribeClusterRoleTokenRequest",
      "name": "查询GooseFS集群角色凭证",
      "output": "DescribeClusterRoleTokenResponse",
      "status": "online"
    },
    "DescribeClusterRoles": {
      "document": "查询GooseFS集群角色",
      "input": "DescribeClusterRolesRequest",
      "name": "查询GooseFS集群角色",
      "output": "DescribeClusterRolesResponse",
      "status": "online"
    },
    "DescribeDataRepositoryTaskStatus": {
      "document": "获取数据流通任务实时状态，用作客户端控制",
      "input": "DescribeDataRepositoryTaskStatusRequest",
      "name": "获取数据流通任务实时状态",
      "output": "DescribeDataRepositoryTaskStatusResponse",
      "status": "online"
    }
  },
  "metadata": {
    "apiVersion": "2022-05-19",
    "api_brief": "介绍如何使用API创建、删除、查询、管理数据加速器GooseFS，为GooseFS服务分发配置，管理存储在GooseFS服务中的文件等。",
    "serviceNameCN": "数据加速器 GooseFS",
    "serviceShortName": "goosefs"
  },
