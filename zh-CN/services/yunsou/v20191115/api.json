        {
          "default": "",
          "document": "指定返回最大篇数，无特殊原因不建议指定",
          "member": "uint64",
          "name": "MaxDocReturn",
          "required": false,
          "type": "int"
        },
        {
          "default": "",
          "document": "是否smartbox检索，0关闭，1打开",
          "member": "uint64",
          "name": "IsSmartbox",
          "required": false,
          "type": "int"
        },
        {
          "default": "",
          "document": "是否打开高红标亮，0关闭，1打开",
          "member": "uint64",
          "name": "EnableAbsHighlight",
          "required": false,
          "type": "int"
        },
        {
          "default": "",
          "document": "指定访问QC纠错业务ID",
